<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui"
          onLoad="Ribbon_Load"
          >
	<ribbon>
		<tabs>
			<tab id="TabSeraing" insertAfterMso="TabMail" label="Seraing"  >
				<group id="GroupIADocs"
					   label="IA.Docs"
					   autoScale="true"
					   centerVertically="true"
					   screentip="Extensions pour IA.Docs"
						 getVisible="GetTabVisibility"
              >
					<button id="Indicateur"
							description="Envoyer le(s) message(s) sélectionné(s) à l'indicateur"
							enabled="true"
							getImage="GetImage"
							size="large"
							onAction="SendToIndicator_Click"
							label="Indicateur"
							screentip="Envoyer le(s) message(s) sélectionné(s) à l'indicateur" />
					<button id="CleanupSentItems"
							description="Nettoyer les messages envoyés à l'indicateur"
							screentip="Nettoyer les messages envoyés à l'indicateur"
							enabled="true"
							getImage="GetImage"
							size="normal"
							onAction="CleanupSentItems_Click"
							label="Nettoyage indicateur" />
					<button id="IndicateurParam"
							description="Voir et éditer les paramètres"
							screentip="Voir et éditer les paramètres"
							enabled="true"
							getImage="GetImage"
							size="normal"
							onAction="EditParameters_Click"
							label="Paramètres indicateur" />
					<button id="IndicateurLog"
							description="Voir le fichier log"
							screentip="Voir le fichier log"
							enabled="true"
							getImage="GetImage"
							size="normal"
							onAction="ViewLog_Click"
							label="Voir fichier log" />

				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>

